{
  "name": "@sujeet-pro/v2",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "x": "npm run clean && npm run check && npm run build",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "gh": "npm run clean && npm run check && npm run build && npm run deploy",
    "deploy": "gh-pages -d dist --dotfiles include",
    "sync": "astro sync",
    "clean": "rimraf .astro dist",
    "check": "npm run check:ts && npm run check:lint && npm run check:format",
    "check:ts": "astro check",
    "check:format": "prettier . --check --ignore-unknown",
    "check:lint": "eslint --ext .js,.cjs,.ts,.astro src",
    "fix": "npm run fix:lint && npm run fix:format",
    "fix:format": "prettier . --write --ignore-unknown",
    "fix:lint": "eslint --ext .js,.cjs,.ts,.astro src --fix"
  },
  "devDependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/prefetch": "^0.4.1",
    "@astrojs/rss": "^3.0.0",
    "@astrojs/sitemap": "^3.0.3",
    "@astrojs/ts-plugin": "^1.3.1",
    "@fontsource-variable/open-sans": "^5.0.18",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "astro": "^3.6.0",
    "astro-icon": "^0.8.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "mdast-util-to-string": "^4.0.0",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2",
    "reading-time": "^1.5.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2"
  }
}