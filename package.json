    "mdast-util-to-string": "^4.0.0",
    "prettier": "^3.2.4",
    "prettier-plugin-astro": "^0.13.0",
    "reading-time": "^1.5.0",
    "rehype-accessible-emojis": "^0.3.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
}
