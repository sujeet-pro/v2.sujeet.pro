{
  "name": "@sujeet-pro/v2",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9.8.1"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "bd": "npm run build && npm run deploy",
    "deploy": "gh-pages -d dist --dotfiles include",
    "check": "npm run check:ts && npm run check:lint && npm run check:format",
    "check:ts": "astro check",
    "check:format": "prettier . --check --ignore-unknown",
    "check:lint": "eslint --ext .js,.cjs,.ts,.astro src",
    "fix": "npm run fix:lint && npm run fix:format",
    "fix:format": "prettier . --write --ignore-unknown",
    "fix:lint": "eslint --ext .js,.cjs,.ts,.astro src --fix"
  },
  "devDependencies": {
    "@astrojs/check": "^0.2.0",
    "@astrojs/ts-plugin": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "astro": "^3.2.2",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "gh-pages": "^6.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "typescript": "^5.2.2"
  }
}