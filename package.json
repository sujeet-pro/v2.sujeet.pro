{
  "name": "@sujeet-pro/v2",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.3"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "x": "npm run clean && npm run check && npm run build",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "gh": "npm run clean && npm run check && npm run build && npm run deploy",
    "sync": "astro sync",
    "clean": "rimraf .astro dist",
    "check": "npm run check:ts && npm run check:lint && npm run check:format",
    "check:ts": "astro check",
    "check:format": "prettier . --check --ignore-unknown",
    "check:lint": "eslint --ext .js,.cjs,.ts,.astro src",
    "fix": "npm run fix:lint && npm run fix:format",
    "fix:format": "prettier . --write --ignore-unknown",
    "fix:lint": "eslint --ext .js,.cjs,.ts,.astro src --fix"
  },
  "devDependencies": {
    "@astrojs/check": "^0.4.1",
    "@astrojs/prefetch": "^0.4.1",
    "@astrojs/rss": "^4.0.4",
